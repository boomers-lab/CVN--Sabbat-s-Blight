<?xml version="1.0" encoding="utf-8" ?>

<Defs>

  <ThingDef ParentName="ApparelNoQualityBase">
    <defName>BP_Backpack</defName>
    <label>Salvaged battleline backpack</label>
    <description>A basic battleline backpack used by the Blood Pact. \n\nThe Pact teaches all of its members to grab anything they can, munitions, food, and more. Backpacks like these allow their warriors to maintain supplies and battle readiness for longer campaigns.</description>
    <techLevel>Industrial</techLevel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedPack</unfinishedThingDef>
      <researchPrerequisites>
			 <li>GW_BP_Plunder</li>
			</researchPrerequisites>
      <recipeUsers>
        <li>CVN_BP_FabricationBench</li>
      </recipeUsers>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Smith</effectWorking>
      <soundWorking>Recipe_Machining</soundWorking>
      <skillRequirements>
        <Crafting>4</Crafting>
      </skillRequirements>
      <displayPriority>340</displayPriority>
    </recipeMaker>
		<stuffCategories>
            <li>Fabric</li>
        </stuffCategories>
		<costStuffCount>35</costStuffCount>
    <graphicData>
      <texPath>Things/Pawn/Humanlike/Apparel/Backpacks/BP_BackpackBattleline_south</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
      <drawSize>1,1</drawSize>
    </graphicData>
    <statBases>
      <WorkToMake>3500</WorkToMake>
      <MaxHitPoints>300</MaxHitPoints>
			<Mass>2</Mass>
			<Flammability>0.5</Flammability>
			<ArmorRating_Sharp>0.3</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.3</ArmorRating_Blunt>
			<ArmorRating_Heat>0.2</ArmorRating_Heat>
			<Insulation_Cold>3</Insulation_Cold>
			<Insulation_Heat>3</Insulation_Heat>
      <EquipDelay>2</EquipDelay>
    </statBases>
    <thingCategories>
      <li>ApparelUtility</li>
    </thingCategories>
		<equippedStatOffsets>
			<MoveSpeed>-0.1</MoveSpeed>
            <CarryingCapacity>15</CarryingCapacity>
              <CarryBulk MayRequire="CETeam.CombatExtended">15</CarryBulk>
		</equippedStatOffsets>
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <scoreOffset>4</scoreOffset>
      <bodyPartGroups>
        <li>Torso</li>
      </bodyPartGroups>
      <layers>
        <li>Belt</li>
      </layers>
      <tags>
        <li>PactBackpack</li>
      </tags>
      <wornGraphicPath>Things/Pawn/Humanlike/Apparel/Backpacks/BP_BackpackBattleline</wornGraphicPath>
      <wornGraphicData>
        <renderUtilityAsPack>true</renderUtilityAsPack>
        <north>
          <offset>(0,0)</offset>
        </north>
        <south>
          <offset>(0,0)</offset>
        </south>
        <east>
          <offset>(0,0)</offset>
          <thin><offset>(0,0)</offset></thin>
          <hulk><offset>(0,0)</offset></hulk>
          <fat> <offset>(0,0)</offset></fat>
        </east>
        <west>
          <offset>(0,0)</offset>
          <thin><offset>(0,0)</offset></thin>
          <hulk><offset>(0,0)</offset></hulk>
          <fat> <offset>(0,0)</offset></fat>
        </west>
        <male>  <scale>(1.1,1.1)</scale></male>
        <female><scale>(1.1,1.1)</scale></female>
        <thin>  <scale>(1.1,1.1)</scale></thin>
        <hulk>  <scale>(1.1,1.1)</scale></hulk>
        <fat>   <scale>(1.1,1.1)</scale></fat>
      </wornGraphicData>
	  <useWornGraphicMask>true</useWornGraphicMask>
    </apparel>
  </ThingDef>
  
  <ThingDef ParentName="ApparelNoQualityBase">
